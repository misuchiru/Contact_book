<%= simple_form_for(contact) do |f| %>
  <%= f.association :district, label: "地區" %>
  <%= f.input :name, label: "名稱" %>
  <%= f.input :cell_phone, label: "手機號碼" %>
  <%= f.input :phone, label: "市話號碼" %>
  <hr>
  <div class="form-inline text-center">
    <%= f.submit "送出", class: "btn btn-primary" %>
    <%= link_to "回內西名單列表", contacts_path, class: "btn btn-default" %>
  </div>
<% end %>
