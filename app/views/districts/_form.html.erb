<%= simple_form_for(district) do |f| %>
  <%= f.association :chapter, label: "支部" %>
  <%= f.input :title, label: "名稱" %>
  <div class="form-inline text-center">
    <%= f.submit "送出", class: "btn btn-primary" %>
    <%= link_to "回地區列表", districts_path, class: "btn btn-default" %>
  </div>
<% end %>
